@model IEnumerable<WebApplicationFinal.Models.Account>
@{
    ViewBag.Title = "ShowTags";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Tags</h2>

<form action="EditAccount" method="POST">
   

    <fieldset>
        <legend>Add tags</legend>
        <div class="form-group row">
            <label for="staticusername" class="col-sm-2 col-form-label">username</label>
          
            <select id="tags">
                @foreach (var details in ViewBag.list1)
                {
                    <option value="@details.tagname">@details.tagname</option>
                }
            </select>
            
</fieldset>
        <button type="submit" class="btn btn-primary">Submit</button>

  
</form>
