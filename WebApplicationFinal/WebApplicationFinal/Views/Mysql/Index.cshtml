@model IEnumerable<WebApplicationFinal.Models.MysqlClass>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

 <body>
     <table>
         <tr>
             <th>ID_admin</th>
             <th>username</th>
             <th>password</th>
         </tr>
         @foreach (WebApplicationFinal.Models.MysqlClass details in Model) { 
            <tr>
                <td>@details.ID_admin</td>
                <td>@details.Username</td>
                <td>@details.Password</td>
            </tr>
         
         }
     </table>
 </body>